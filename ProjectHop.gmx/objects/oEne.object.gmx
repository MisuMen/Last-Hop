{
 hsp = lengthdir_x(32,abs((180*sign(oPlyer.image_xscale-1))+r_m));
 v = lengthdir_y(32,abs((180*sign(oPlyer.image_xscale-1))+r_m));
 ROT += 16*sign(image_xscale);
}
