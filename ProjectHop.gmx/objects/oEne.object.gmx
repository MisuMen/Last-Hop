{
 while(!place_meeting(x+sign(hsp),y,oBox)){x += sign(hsp);}
 if launch==0 dir *= -1;
 //if launch==1 {instance_destroy(); ++global.MOB_KILL; score+=earn}
}

if (place_meeting(x,y+vsp,oBox))&amp;&amp; launch==0
{
 while(!place_meeting(x,y+sign(vsp),oBox)){y += sign(vsp);}
 vsp = 0;
}
//////

if (place_meeting(x+hsp,y,oBox2))&amp;&amp; dir==sign(hsp)&amp;&amp;(launch==0)
{
 while(!place_meeting(x+sign(hsp),y,oBox2)){x += sign(hsp);}
 if launch==0 dir *= -1;
 //if launch==1 {instance_destroy(); ++global.MOB_KILL; score+=earn}
}
x+=hsp

if (place_meeting(x,y+vsp,oBox2))&amp;&amp;(launch==0)
{
 while(!place_meeting(x,y+sign(vsp),oBox2)){y += sign(vsp);}
 vsp = 0;
}
y+=vsp+v
}
</string>
