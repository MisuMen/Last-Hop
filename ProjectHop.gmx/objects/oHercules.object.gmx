        <arguments>
          <argument>
            <kind>1</kind>
            <string>///movement and physics

if (oButtons.INVENTORY==0)&amp;&amp;(instance_number(oDialogue)==0)
{
if DIE == 1 exit
////Move left or right
var mm = move_right - move_left;
if move==12 {mm=temp_m}
hsp =  (mm * move);
////Animation for HSP
//if hsp != 0 {if SPR==spr_player {IMG+= (0.3); if (IMG&gt;6) IMG=0}}else{if air==0 IMG=0}
//if SPR==spr_jump {if IMG &lt; 4 IMG +=  (0.5)}
////////////////PHYSICS////////////////////////
if (vsp &lt; 16) vsp +=  (grav);

if (place_meeting(x,y+1,oBox))
{
var pp = instance_place(x,y+1,oBox);
if (y+120 &lt; pp.y)
{
 vsp =  (jk * -jump);
 //if SPR==spr_jump SPR = spr_player
 air=0
}
}//
if (place_meeting(x,y+1,oBox2))
{
 vsp =  (jk * -jump);
 //if SPR==spr_jump SPR = spr_player
 air=0
}//
/////////////////COLLISION////////////////////
if oPlyer.SUMMON==0
{
var ystep1, ystep2;

if (place_meeting(x+hsp,y,oBox))
{
 var oo = instance_place(x+hsp,y,oBox);
 if (y+120 &lt; oo.y) &amp;&amp; vsp&gt;0
 {
 ystep1=0;
 while(place_meeting(x+hsp,y-ystep1,oBox)&amp;&amp;(ystep1 &lt;= abs(2*hsp))){ystep1+=1}
 if (place_meeting(x+hsp,y-ystep1,oBox))
 {
 while(!place_meeting(x+sign(hsp),y,oBox)){x += sign(hsp);}
 hsp = 0;
 }
 else{y-=ystep1}//
 }
}
if (place_meeting(x+hsp,y,oBox2))
{
 ystep2=0;
 while(place_meeting(x+hsp,y-ystep2,oBox2)&amp;&amp;(ystep2 &lt;= abs(2*hsp))){ystep2+=1}
 if (place_meeting(x+hsp,y-ystep2,oBox2))
 {
 while(!place_meeting(x+sign(hsp),y,oBox2)){x += sign(hsp);}
 hsp = 0;
 }
 else{y-=ystep2}
}
x+=hsp ///&lt;------------------------------ X move
/////////////////
if !place_meeting(x,y,oBox) &amp;&amp; vsp &gt;= 0 &amp;&amp; place_meeting(x,y+2+abs(hsp),oBox)
{while(!place_meeting(x,y+1,oBox)) {y += 1;}}
/////////////////
if !place_meeting(x,y,oBox2) &amp;&amp; vsp &gt;= 0 &amp;&amp; place_meeting(x,y+2+abs(hsp),oBox2)
{while(!place_meeting(x,y+1,oBox2)) {y += 1;}}
/////////////////
if (place_meeting(x,y+vsp,oBox))
{
 var ii = instance_place(x,y+vsp,oBox);
 if (y+120 &lt; ii.y) &amp;&amp; vsp&gt;0
 {
 while(!place_meeting(x,y+sign(vsp),oBox)){y += sign(vsp);}
 vsp = 0;
 jk=0
 }
}
/////////////Full Collision/////////////////////
/////////////////
if (place_meeting(x,y+vsp,oBox2))
{
 while(!place_meeting(x,y+sign(vsp),oBox2)){y += sign(vsp);}
 vsp = 0;
 jk=0
}
//////////////Enemy Collision///////////////////
if (place_meeting(x+hsp,y,oBad1))
{
 while(!place_meeting(x+sign(hsp),y,oBad1)){x += sign(hsp);}
 with(instance_place(x,y+1,oBad1)){alarm[1]=60; launch=1}
 exit
}
if (place_meeting(x,y+abs(vsp),oBad1))
{
 while(!place_meeting(x,y+1,oBad1)){y += 1;}
 with(instance_place(x,y+1,oBad1)){instance_destroy();global.MOB_KILL++}
 vsp = -jump;
 score+=100
 exit
}
}
y+=vsp
}
</string>
