      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///movement and physics
if (SUMMON) exit
if (oButtons.INVENTORY==0)&amp;&amp;(instance_number(oDialogue)==0)&amp;&amp;(instance_number(oHercules)==0)
{
var slope = 0;

if fall == 1 {y+=10}
if DIE == 1 exit

var mm = move_right - move_left;
if move==12 {mm=temp_m}
hsp = (mm * move);

if hsp != 0 {if SPR==spr_player {IMG+=(0.3); if (IMG&gt;15) IMG=0}}else{if air==0 IMG=0}
if SPR==spr_jump {if IMG &lt; 4 IMG += (0.5)}
////////////////PHYSICS////////////////////////
if (vsp &lt; 16)&amp;&amp;(hover==0) vsp += (grav);
var rk = (move_down - move_up);

if (hover){vsp = rk*move}
///ROLLING/////
if (ROLL)
{
 DIR-=15*(image_xscale)
 DIR = DIR mod 360;
}
//////////////If Stepping on ground//////////////
if (place_meeting(x,y+1,oBox))&amp;&amp;(hover==0)
{
var pp = instance_place(x,y+1,oBox);
if (y+120 &lt; pp.y)
{
 if (vsp&gt;0)&amp;&amp;(air&gt;0){instance_create(x,y+120,oDust);}
 vsp = (jk * -jump);
 if (SPR==spr_jump)&amp;&amp;(air&lt;2) {SPR = spr_player; air=0}///Bug
 if (ROLL &amp;&amp; DIR==0)
 {
    pressed=0
    move = 6
    SPR = spr_player
    IMG = 0
    air=0
    temp_m=0
    ROLL=0
 }
}
}//
if (place_meeting(x,y+1,oBox2))&amp;&amp;(hover==0)
{
 if (vsp&gt;0)&amp;&amp;(air&gt;0){instance_create(x,y+120,oDust);}
 vsp =  (jk * -jump);
 if (SPR==spr_jump)&amp;&amp;(air&lt;2) {SPR = spr_player;  air=0}///Bug
 if (ROLL &amp;&amp; DIR==0)
 {
    pressed=0
    move = 6
    SPR = spr_player
    IMG = 0
    air=0
    temp_m=0
    ROLL=0
 }
}//
/////////////////COLLISION////////////////////
var ystep1, ystep2;

if (place_meeting(x+hsp,y,oBox))
{
 var oo = instance_place(x+hsp,y,oBox);
 if (y+120 &lt; oo.y) &amp;&amp; vsp&gt;0
 {
 ystep1=0;
 while(place_meeting(x+hsp,y-ystep1,oBox)&amp;&amp;(ystep1 &lt;= abs(2*hsp))){ystep1+=1}
 if (place_meeting(x+hsp,y-ystep1,oBox))
 {
 while(!place_meeting(x+sign(hsp),y,oBox)){x += sign(hsp);}
 hsp = 0;
 }
 else{y-=ystep1}//
 }
}
if (place_meeting(x+hsp,y,oBox2))
{
 ystep2=0;
 while(place_meeting(x+hsp,y-ystep2,oBox2)&amp;&amp;(ystep2 &lt;= abs(2*hsp))){ystep2+=1}
 if (place_meeting(x+hsp,y-ystep2,oBox2))
 {
 while(!place_meeting(x+sign(hsp),y,oBox2)){x += sign(hsp);}
 hsp = 0;
 }
 else{y-=ystep2}
}
x+=hsp ///&lt;------------------------------ X move
/////////////////
if !place_meeting(x,y,oBox) &amp;&amp; vsp &gt;= 0 &amp;&amp; place_meeting(x,y+2+abs(hsp),oBox)
{while(!place_meeting(x,y+1,oBox)) {y += 1;}}
/////////////////
if !place_meeting(x,y,oBox2) &amp;&amp; vsp &gt;= 0 &amp;&amp; place_meeting(x,y+2+abs(hsp),oBox2)
{while(!place_meeting(x,y+1,oBox2)) {y += 1;}}

/////////////////y collision///////////////////
if (place_meeting(x,y+vsp,oBox))
{
 var ii = instance_place(x,y+vsp,oBox);
 if (y+120 &lt; ii.y) &amp;&amp; vsp&gt;0
 {
 while(!place_meeting(x,y+sign(vsp),oBox)){y += sign(vsp);}
 //instance_create(x,y+120,oDust);
 vsp = 0;
 jk=0
 }
}
/////////////Full Collision/////////////////////
if (place_meeting(x,y+vsp,oBox2))
{
 while(!place_meeting(x,y+sign(vsp),oBox2)){y += sign(vsp);}
 //instance_create(x,y+120,oDust);
 vsp = 0;
 jk=0
}
//////////////Enemy Collision///////////////////
if (place_meeting(x+hsp,y,oBad1))
{
 while(!place_meeting(x+sign(hsp),y,oBad1)){x += sign(hsp);}
 if IMG==1 
 {
  var ii = instance_place(x,y+1,oBad1);
  instance_create(ii.x,ii.y,oCross);
  shake=16
  with(ii){alarm[1]=60; oButtons.mot1=1; launch=1}
 }
 else
 {
  global.HP-=DAMAGE;
  if global.HP&lt;=0
  {
    alarm[1]=40;
    oButtons.LIFE_CHECK=lives
    DIE = 1;
  }
 }
 exit
}
if (place_meeting(x,y+abs(vsp),oBad1))
{
 while(!place_meeting(x,y+1,oBad1)){y += 1;}
 with(instance_place(x,y+1,oBad1)){instance_destroy();global.MOB_KILL++}
 instance_create(x,y+120,oDust);
 instance_create(x,y+120,oCross);
 vsp = -jump;
 score+=100
 shake=8
 exit
}
y+=vsp

move_down = 0
 move_up = 0
}
</string>
